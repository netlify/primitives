  "devDependencies": {
    "@types/node": "^20.5.1",
    "tsup": "^8.0.0",
    "vite": "^6.3.4",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "@netlify/dev": "2.2.0",
    "chalk": "^5.4.1"
  },
  "peerDependencies": {
    "vite": "^6.2"
  }

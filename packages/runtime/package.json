{
  "name": "@netlify/runtime",
  "version": "4.0.1",
  "description": "Netlify compute runtime",
  "type": "module",
  "engines": {
    "node": ">=20.6.1"
  },
  "main": "./dist/main.js",
  "exports": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup-node",
    "prepack": "pnpm run build",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:ci": "pnpm run build && vitest run",
    "dev": "tsup-node --watch",
    "publint": "npx -y publint --strict"
  },
  "keywords": [],
  "license": "MIT",
  "repository": "netlify/primitives",
  "bugs": {
    "url": "https://github.com/netlify/primitives/issues"
  },
  "author": "Netlify Inc.",
  "dependencies": {
    "@netlify/blobs": "workspace:*",
    "@netlify/cache": "workspace:*",
    "@netlify/runtime-utils": "workspace:*",
    "@netlify/types": "workspace:*"
  },
  "devDependencies": {
    "@netlify/dev-utils": "workspace:*",
    "@types/node": "^20.17.57",
    "tsup": "^8.0.0",
    "vitest": "^3.0.0"
  }
}

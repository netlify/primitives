  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@netlify/dev-utils": "2.1.0",
    "@types/http-proxy": "^1.17.16",
    "@types/jsonwebtoken": "9.0.9"
  },
  "dependencies": {
    "cookie": "^1.0.2",
    "jsonwebtoken": "9.0.2",
    "netlify-redirect-parser": "^14.4.0",
    "netlify-redirector": "^0.5.0"
  }

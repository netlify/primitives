{
  "name": "@netlify/ai-gateway",
  "version": "1.0.0",
  "description": "Netlify AI Gateway integration for local development",
  "type": "module",
  "engines": {
    "node": ">=20.6.1"
  },
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/main.d.cts",
        "default": "./dist/main.cjs"
      },
      "import": {
        "types": "./dist/main.d.ts",
        "default": "./dist/main.js"
      },
      "default": {
        "types": "./dist/main.d.ts",
        "default": "./dist/main.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup-node",
    "dev": "tsup-node --watch",
    "prepack": "npm run build",
    "test": "run-s build test:ci",
    "test:dev": "run-s build test:dev:*",
    "test:ci": "run-s build test:ci:*",
    "test:dev:vitest": "vitest",
    "test:dev:vitest:watch": "vitest watch",
    "test:ci:vitest": "vitest run",
    "publint": "npx -y publint --strict"
  },
  "dependencies": {
    "@netlify/api": "^14.0.4"
  },
  "devDependencies": {
    "@types/node": "20.14.15",
    "npm-run-all2": "^7.0.2",
    "tsup": "8.5.0",
    "typescript": "5.5.4",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "@netlify/api": ">=14.0.0"
  }
}

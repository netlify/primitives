{
  "name": "@netlify/edge-functions",
  "version": "2.12.0",
  "description": "TypeScript utilities for interacting with Netlify Edge Functions",
  "type": "module",
  "engines": {
    "node": "^14.16.0 || >=16.0.0"
  },
  "main": "./dist/main.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "./dev": {
      "types": "./dist-dev/main.d.ts",
      "default": "./dist-dev/main.js"
    },
    "./version": {
      "types": "./dist/version.d.mts",
      "default": "./dist/version.mjs"
    }
  },
  "types": "./dist/main.d.ts",
  "files": [
    "dist/**/*",
    "dist-dev/**/*",
    "deno/**/*",
    "server.d.ts"
  ],
  "scripts": {
    "build": "tsup-node",
    "prepack": "npm run build",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:ci": "npm run build && vitest run",
    "dev": "tsup-node --watch",
    "publint": "npx -y publint --strict"
  },
  "keywords": [],
  "license": "MIT",
  "repository": "netlify/primitives",
  "bugs": {
    "url": "https://github.com/netlify/primitives/issues"
  },
  "author": "Netlify Inc.",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@netlify/types": "1.2.0"
  },
  "dependencies": {
    "@netlify/dev-utils": "3.0.0",
    "@netlify/edge-bundler": "^14.0.4",
    "@netlify/runtime-utils": "2.0.0",
    "get-port": "^7.1.0"
  }
}

{
  "name": "@netlify/edge-functions",
  "version": "2.14.1",
  "description": "TypeScript utilities for interacting with Netlify Edge Functions",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "./dist/main.js",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "default": "./dist/main.js"
    },
    "./dev": {
      "types": "./dist-dev/node/main.d.ts",
      "default": "./dist-dev/node/main.js"
    },
    "./version": {
      "types": "./dist/version.d.ts",
      "default": "./dist/version.js"
    }
  },
  "types": "./dist/main.d.ts",
  "files": [
    "dist/**/*",
    "dist-dev/**/*",
    "deno/**/*",
    "server.d.ts"
  ],
  "scripts": {
    "build": "tsup-node",
    "prepack": "pnpm run build",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:ci": "pnpm run build && vitest run",
    "dev": "tsup-node --watch",
    "publint": "publint --strict"
  },
  "keywords": [
    "netlify",
    "edge functions"
  ],
  "license": "MIT",
  "repository": "netlify/primitives",
  "bugs": {
    "url": "https://github.com/netlify/primitives/issues"
  },
  "author": "Netlify Inc.",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@netlify/types": "workspace:*",
    "tsup": "^8.0.0"
  },
  "dependencies": {
    "@netlify/dev-utils": "workspace:*",
    "@netlify/edge-bundler": "^14.0.5",
    "@netlify/edge-functions-bootstrap": "^2.13.0",
    "@netlify/runtime-utils": "workspace:*",
    "get-port": "^7.1.0"
  }
}

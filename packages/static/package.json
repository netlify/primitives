{
  "name": "@netlify/static",
  "version": "1.1.3",
  "description": "TypeScript implementtion of Netlify's static file serving logic",
  "type": "module",
  "engines": {
    "node": "^14.16.0 || >=16.0.0"
  },
  "main": "./dist/main.js",
  "exports": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "files": [
    "dist/**/*",
    "server.d.ts"
  ],
  "scripts": {
    "build": "tsup-node",
    "prepack": "npm run build",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:ci": "npm run build && vitest run",
    "dev": "tsup-node --watch",
    "publint": "npx -y publint --strict"
  },
  "keywords": [],
  "license": "MIT",
  "repository": "netlify/primitives",
  "bugs": {
    "url": "https://github.com/netlify/primitives/issues"
  },
  "author": "Netlify Inc.",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@netlify/dev-utils": "2.1.1",
    "@types/mime-types": "^2.1.4"
  },
  "dependencies": {
    "mime-types": "^2.1.35"
  }
}

{
  "name": "@netlify/static",
  "version": "1.1.2",
  "description": "JavaScript implementtion of Netlify's static file serving logic",
  "type": "module",
  "engines": {
    "node": "^14.16.0 || >=16.0.0"
  },
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/main.d.cts",
        "default": "./dist/main.cjs"
      },
      "import": {
        "types": "./dist/main.d.ts",
        "default": "./dist/main.js"
      },
      "default": {
        "types": "./dist/main.d.ts",
        "default": "./dist/main.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "server.d.ts"
  ],
  "scripts": {
    "build": "tsup-node",
    "prepack": "npm run build",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:ci": "npm run build && vitest run",
    "dev": "tsup-node --watch"
  },
  "keywords": [],
  "license": "MIT",
  "repository": "netlify/primitives",
  "bugs": {
    "url": "https://github.com/netlify/primitives/issues"
  },
  "author": "Netlify Inc.",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@netlify/dev-utils": "2.1.0",
    "@types/mime-types": "^2.1.4"
  },
  "dependencies": {
    "mime-types": "^2.1.35"
  }
}

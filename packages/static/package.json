{
  "name": "@netlify/static",
  "version": "3.0.1",
  "description": "TypeScript implementtion of Netlify's static file serving logic",
  "type": "module",
  "engines": {
    "node": ">=20.6.1"
  },
  "main": "./dist/main.js",
  "exports": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "files": [
    "dist/**/*",
    "server.d.ts"
  ],
  "scripts": {
    "build": "tsup-node",
    "prepack": "pnpm run build",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:ci": "pnpm run build && vitest run",
    "dev": "tsup-node --watch",
    "publint": "publint --strict"
  },
  "keywords": [],
  "license": "MIT",
  "repository": "netlify/primitives",
  "bugs": {
    "url": "https://github.com/netlify/primitives/issues"
  },
  "author": "Netlify Inc.",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "@netlify/dev-utils": "workspace:*",
    "@types/mime-types": "^2.1.4",
    "tsup": "^8.0.0",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "mime-types": "^3.0.0"
  }
}
